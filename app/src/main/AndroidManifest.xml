<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.canyinghao.canaccess">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:name=".App"
        android:icon="@mipmap/ic_launcher" android:theme="@style/Theme.DesignDemo">



        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.CheeseDetailActivity" />


        <service android:label="@string/acc_service_name" android:name=".service.MyAccessibilityService" android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>
            <meta-data android:name="android.accessibilityservice" android:resource="@xml/phone_accessibility" />
        </service>
    </application>

</manifest>
